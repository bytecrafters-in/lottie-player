{
	"compilerOptions": {
		"target": "es5",
		"module": "esnext",
		"jsx": "react-jsx",
		"declaration": true,
		"declarationDir": "./dist/types",
		"outDir": "./dist",
		"moduleResolution": "node",
		"strict": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"esModuleInterop": true
	},
	"include": ["src"],
	"exclude": ["node_modules", "dist"]
}
